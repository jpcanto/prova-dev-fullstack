{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Iniciar o Programa",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "runtimeExecutable": "yarn",
      "runtimeArgs": [
        "dev"
      ],
      "program": "${workspaceFolder}/src/shared/infra/http/server.ts",
      "outFiles": [
        "${workspaceFolder}/**/*.js"
      ],
      "console": "integratedTerminal",
      "autoAttachChildProcesses": true
    }
  ]
}
